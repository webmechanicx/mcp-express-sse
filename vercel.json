{
    "version": 2,
    "builds": [
        {
            "src": "build/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["build/**"] }
        }
    ],
    "rewrites": [
        { 
            "source": "/(.*)", 
            "destination": "/" 
        }
    ] 

}